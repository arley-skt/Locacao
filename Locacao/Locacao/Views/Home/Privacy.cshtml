@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use este espaço para resumir sua política de privacidade e uso de cookies.</p>

<form>
    <div class="form-group">
        <label for="exampleInputEmail1">Login</label>
        <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="email">
        <small id="emailHelp" class="form-text text-muted">
            Nunca compartilharemos seu email com mais ninguém.
        </small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Senha</label>
        <input name="Senha" type="password" class="form-control" id="exampleInputPassword1" placeholder="Senha">
    </div>

    <button id="Teste" class="btn btn-primary">Entrar</button> <a href="/Login/Index" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Novo Usuario</a>
</form>


@section  Scripts{
    <script type="text/javascript">

        $('#Teste').click(function () {
            event.preventDefault();
            alert("fui clicado");
        }

        );
    </script>
 }



